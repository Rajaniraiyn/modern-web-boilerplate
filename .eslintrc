{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true,
    "browser": true
  },
  "plugins": ["svelte3", "@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "rules": {},
  "settings": {
    "svelte3/typescript": true
  }
}
